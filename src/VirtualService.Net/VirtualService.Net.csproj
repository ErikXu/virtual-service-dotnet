<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>virtual-service-dotnet</AssemblyName>
    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
    <UserSecretsId>3a957a64-53dd-4713-b82e-5871c7c2b034</UserSecretsId>
  </PropertyGroup>

  <PropertyGroup>
    <KubeOpsSkipDockerfile>true</KubeOpsSkipDockerfile>
  </PropertyGroup>

  <PropertyGroup>
    <KubeOpsSkipCrds>true</KubeOpsSkipCrds>
  </PropertyGroup>

  <PropertyGroup>
    <KubeOpsSkipRbac>true</KubeOpsSkipRbac>
  </PropertyGroup>

  <PropertyGroup>
    <KubeOpsSkipOperator>true</KubeOpsSkipOperator>
  </PropertyGroup>

  <PropertyGroup>
    <KubeOpsSkipInstaller>true</KubeOpsSkipInstaller>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="KubeOps" Version="6.0.18" />
  </ItemGroup>

</Project>